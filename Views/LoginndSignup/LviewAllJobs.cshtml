@{
    ViewBag.Title = "LviewAllJobs";
}

@using fViews.Models;

<link rel="stylesheet" type="text/css" href="~/Content/bootstrap/css/bootstrap.css" />
<link rel="stylesheet" type="text/css" href="~/Content/bootstrap/css/bootstrap.min.css" />

<script type="text/javascript" src="~/Content/jquery/jquery-2.1.4.min.js"></script>
<script type="text/javascript" src="~/Content/bootstrap/js/bootstrap.min.js"></script>

<html>
<body background="~/Content/Wh.jpg">

	<h2>Hello</h2>

	<div class="container">

		<div class="row">

			@foreach (Job data in ViewData.Model)
			{

				<div class="col-sm-4">
					<div class="panel panel-primary">
						<div class="panel-heading">
							<h3 class="panel-title">@(data.jName)</h3>
						</div>

						<div class="panel-body">
							<ul>
								<li>
									Posted By : @(data.jPosterName)
								</li>
							</ul>

							<ul>
								<li>
									Rate : @(data.jRate)
								</li>
							</ul>

							<ul>
								<li>
									Duration : @(data.jDuration)
								</li>
							</ul>

							<ul>
								<p>
									Discription  : @(data.jDiscription)
								</p>
							</ul>


						</div>

						<div class="panel-footer clearfix">
							<div class="pull-right">
								<a href="/LoginndSignup/Bid" class="btn btn-primary">Bid Now</a>

							</div>
						</div>

					</div>
				</div>




			}

		</div>


	</div>

</body>
</html>